{
  "name": "portfolio_company",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "vite build && vite-bundle-visualizer",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --host",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test:run",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@fontsource/urbanist": "^5.2.6",
    "@lottiefiles/dotlottie-react": "^0.17.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-scroll": "^1.8.10",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.12",
    "lottie-react": "^2.4.1",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.66.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "react-scroll": "^1.9.3",
    "react-shadow": "^20.6.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  }
}
